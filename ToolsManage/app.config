<?xml version="1.0"?>
<configuration>
  <connectionStrings>
    <add name="ToolsManage" connectionString="Data Source=.;Initial Catalog=ToolsManage;user=sa;password=123456;" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <appSettings>


    <!--IsUserPowReader PowReaderIP PowReaderToIP  PowReaderToPort OutId1 InId1 -->
    
    <!--半有源读写器 是否启用-->
    <add key="IsUserPowReader" value="未启用"/>
    <!--半有源读写器 IP地址-->
    <add key="PowReaderIP" value="192.168.1.254"/>
    <!--半有源读写器 目标IP 即PC端-->
    <add key="PowReaderToIP" value="192.168.1.108"/>
    <!--半有源读写器 目标端口 即PC端-->
    <add key="PowReaderToPort" value="32500"/>
    <!--半有源读写器 出入库识别 室外激活ID 1# 2# 3# 4#-->
    <add key="OutId1" value="182"/>
    <add key="OutId2" value=""/>
    <add key="OutId3" value=""/>
    <add key="OutId4" value=""/>
    <!--半有源读写器 出入库识别 室内激活ID 1# 2# 3# 4#-->
    <add key="InId1" value="100"/>
    <add key="InId2" value=""/>
    <add key="InId3" value=""/>
    <add key="InId4" value=""/>

    <!--半有源读写器 盘库激活器 ID--> 
    <add key="MarkToolId" value=""/>
    <!--盘库时长-->
    <add key="MarkTmie" value="5"/>

    <!--开门 报班组名称 Volume  VideoIP  -->
    <add key="VoiceGroupName" value="某某智能工器具库房"/>
    <!--语音 音量 -->
    <add key="Volume" value="5"/>
    <!--硬盘录像机IP -->
    <add key="VideoIP" value="192.168.1.64"/>
    <!--硬盘录像机端口 -->
    <add key="VideoPort" value="8000"/>
    <!--硬盘录像机用户名 -->
    <add key="VideoUserName" value="admin"/>
    <!--硬盘录像机密码 -->
    <add key="VideoPSW" value="admin12345"/>
    <!--摄像头数量 -->
    <add key="CameraCount" value="2"/>
    <!--盘库功能是否启用 -->
    <add key="MarkUsing" value="启用"/>
    <!--是否485新风-->
    <add key="Is485XF" value="false"/>
    <!--是否启动计划任务-->
    <add key="IsStartPlanTask" value="false"/>
    
  </appSettings>
  
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup></configuration>
